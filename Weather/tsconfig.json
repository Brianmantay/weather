{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "lib": [ "es2015", "dom" ],
    "types": [
      "webpack-env",
      "googlemaps",
      "markerclustererplus"
    ],
    "experimentalDecorators": true
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
